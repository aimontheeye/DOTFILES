# Keybinds
bind c exec kitty
bind Escape abort
bind w exec firefox

# Startup
set padding 10 10 10 10
exec feh --bg-fill ~/Pictures/7.jpg
exec xrandr --output Virtual1 --mode 1680x1050 --rate 144.00
exec pulseaudio --start

# Workspaces
exec rpws init 4 -a
definekey top C-M-1 rpws1
definekey top C-M-2 rpws2
definekey top C-M-3 rpws3
definekey top C-M-4 rpws4
definekey top C-M-Left rpwsp
definekey top C-M-Right rpwsn
